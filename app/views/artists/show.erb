<h2><%= @artist.name %></h2>
<ul>
  <li><strong>Songs: </strong></li>
    <ul>
      <% @songs.each do |song| %>
       <li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
      <% end %>
    </ul>

  <li><strong>Genres: </strong></li>
    <ul>
      <% @genres.each do |genre| %>
        <li><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></li>
      <% end %>
    </ul>
</ul>